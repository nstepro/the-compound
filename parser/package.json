{
  "name": "compound-parser",
  "version": "1.0.0",
  "description": "Parser for vacation compound Google Doc to structured JSON",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "parse": "node src/index.js",
    "debug": "LOG_LEVEL=debug node src/debug.js",
    "clean-logs": "node -e \"const fs = require('fs'); const path = require('path'); if (fs.existsSync('./logs')) { fs.rmSync('./logs', { recursive: true, force: true }); console.log('Logs cleaned'); } else { console.log('No logs to clean'); }\"",
    "test-enrichment": "node src/test-enrichment.js",
    "test-google-places": "node test-google-places.js"
  },
  "dependencies": {
    "@langchain/openai": "^0.2.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "dotenv": "^16.3.1",
    "googleapis": "^126.0.1",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.2.0",
    "openai": "^4.20.1",
    "turndown": "^7.1.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "parser",
    "google-docs",
    "openai",
    "langchain",
    "vacation-compound"
  ],
  "author": "",
  "license": "MIT"
}
